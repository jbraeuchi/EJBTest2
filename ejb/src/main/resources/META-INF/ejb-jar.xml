<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee"
         version="3.1"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd">

    <enterprise-beans>
        <session>

            <ejb-name>SLBean</ejb-name>
            <ejb-class>ch.brj.ejb.interceptors.SLBean</ejb-class>

            <env-entry>
                <description>The Greeting</description>
                <env-entry-name>greeting</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>Hello from Environment</env-entry-value>
            </env-entry>
        </session>
    </enterprise-beans>
</ejb-jar>
